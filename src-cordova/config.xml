<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jamalong.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>JamAlong</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <icon density="mdpi" height="1024" platform="ios" src="../src/assets/apple.png" width="1024" />
    <icon density="mdpi" height="1024" platform="android" src="../src/assets/apple.png" width="1024" />
    <hook src="../node_modules/vue-cli-plugin-cordova/serve-config-hook.js" type="after_prepare" />
    <hook src="../node_modules/vue-cli-plugin-cordova/serve-config-hook.js" type="after_prepare" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <feature name="Camera">
    <param name="ios-package" value="CDVCamera" />
</feature>
    </platform>
    <feature name="Camera">
    <param name="ios-package" value="CDVCamera" />
</feature>
    <plugin name="cordova-plugin-whitelist" spec="1" />
</widget>
